{
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.codeActionsOnSave": ["source.fixAll.eslint"],
  "stylelint.enable": true,
  "emmet.syntaxProfiles": {
    "jsx": {
      "markup.attributes": {
        "class*": "className"
      },
      "markup.valuePrefix": {
        "class*": "styles"
      }
    }
  }
}
